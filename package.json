{
    "name": "couchset",
    "version": "0.0.9",
    "description": "Couchbase ORM & Automatic GraphQL API(Resolver/Queries)",
    "private": false,
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "docs/",
        "dist/",
        "register/",
        "LICENSE"
    ],
    "scripts": {
        "lint": "tslint \"src/**/*.ts\" --project tsconfig.json",
        "build": "rimraf dist && ./node_modules/.bin/tsc --skipLibCheck",
        "test": "mocha src/index.test.ts --exit",
        "test:auto": "mocha src/automation.test.ts --exit",
        "prepublishOnly": "npm run build",
        "eslint": "eslint ./src --fix --ext=ts"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "npm run eslint"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "engines": {
        "node": ">=4.2.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/couchset/couchset.git"
    },
    "keywords": [
        "couchset",
        "couchbase",
        "couchset",
        "database",
        "nosql",
        "sql",
        "data",
        "migrations",
        "mongodb",
        "orm",
        "mongoose",
        "redis"
    ],
    "author": {
        "name": "Ceddy Muhoza",
        "email": "ceddymuhoza@gmail.com",
        "url": "https://ceddy.org"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/couchset/couchset/issues"
    },
    "homepage": "https://github.com/couchset/couchset#readme",
    "devDependencies": {
        "@types/chai": "^4.2.14",
        "@types/couchbase": "^2.4.4",
        "@types/debug": "^4.1.7",
        "@types/express": "^4.17.13",
        "@types/lodash": "^4.14.168",
        "@types/mocha": "^8.2.0",
        "@types/node": "^10.0.3",
        "@types/source-map-support": "^0.4.0",
        "@types/uuid": "^8.3.0",
        "@typescript-eslint/eslint-plugin": "^3.4.0",
        "@typescript-eslint/parser": "^3.4.0",
        "chai": "^4.2.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-json": "^2.1.1",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-simple-import-sort": "^5.0.2",
        "eslint-plugin-typescript": "^0.14.0",
        "husky": "^4.2.5",
        "lint-staged": "^10.1.7",
        "mocha": "^8.2.1",
        "prettier": "^2.0.5",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^2.5.4",
        "ts-node": "^9.1.1",
        "tslint": "^5.11.0",
        "tslint-config-standard": "^8.0.1",
        "typescript": "^4.4.3"
    },
    "peerDependencies": {
        "typescript": ">=2.0"
    },
    "dependencies": {
        "couchbase": "^3.2.1",
        "debug": "^4.3.1",
        "dotenv": "^10.0.0",
        "graphql": "^15.6.0",
        "graphql-redis-subscriptions": "^2.4.0",
        "graphql-tag": "^2.12.5",
        "graphql-type-json": "^0.3.2",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.20",
        "ts-morph": "^12.0.0",
        "type-graphql": "^1.1.1",
        "uuid": "^8.3.2"
    }
}